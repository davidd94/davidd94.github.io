commandHook(contactRequest, function (e, response) {
this.command.waitAfter = isItAgainGetTime(response, 2000);
const timeToDelay = isItAgainGetTime(response, 2500)
waitToElement("#dc-output form", function () {
const formDelete = document.querySelector("#dc-output form");
formDelete.remove();
const contactText = document.querySelector("#dc-output .ichr-output-text");
const form = document.createElement("form");
form.id = "formContact";
form.action = "https://webto.salesforce.com/servlet/servlet.WebToCase?encoding=UTF-8";
form.method = "POST";
form.innerHTML = ` <input type=hidden name="orgid" value="00DE0000000ZFsW">        
<input type="hidden" name="retURL" value="http://enfamil.com">
<input type=hidden name="ownerid" value="00B0y00000A88SJEAZ">
<label for="subject">Full Name:</label><input  id="subject" maxlength="80" name="subject" size="20" type="text" />
<label for="email">Email Address:</label><input  id="email" maxlength="80" name="email" size="20" type="text" />
<label for="description">Comments:</label><textarea name="description"></textarea><br>
<input type="hidden"  id="external" name="external" value="1" />`
form.style.display = "none";
contactText.after(form);
const newSubmit = createElementAndAddAtribute("input", "type", "submit");
newSubmit.setAttribute("form", "formContact");
newSubmit.name = "submit";
newSubmit.value = "Send";
newSubmit.style.display = "none";
form.after(newSubmit);
addCancelButton("Cancel", IWVA_showWelcome, newSubmit);
const cancelButton = document.querySelector("#dc-output .iwsaas-form-cancel");
cancelButton.style.display = "none";
setTimeout(function () {
const subbmitButton = document.querySelector("#dc-output .iwsaas-form-submit");
subbmitButton.remove();
const inputForm = document.querySelectorAll("#dc-output form input:not([type='hidden'])");
for (const input of inputForm) {
input.addEventListener('input', function () {
if (inputForm[0].value.trim() && inputForm[1].value.trim()) {
newSubmit.style.visibility = "visible";
newSubmit.style.opacity = "1";
} else {
newSubmit.style.visibility = "";
newSubmit.style.opacity = "";
}
});
}
form.style.display = "";
newSubmit.style.display = "";
cancelButton.style.display = "";
scrollToEnd();
}, timeToDelay);
}, 10);
});
