return match.replace(new RegExp('#' + dockerId, 'gi'), '');
});
v118 = v118.replace(new RegExp('#' + dockerId + '[\\s]+(@[^{]*)', 'gi'), '$1');//replace #dockerId @foo to @foo
v118 = v118.replace('#' + dockerId +' }', '}'); // remove #dockerId from end media queries
