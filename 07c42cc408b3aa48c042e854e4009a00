const generateSvg = () => {
const size = 100;
const halfSize = size / 2;
const mainRadius = 38;
const prevStroke = 5; // SASS
const prevRadius = Math.ceil(mainRadius - (halfSize - mainRadius) - prevStroke / 2); // SASS
const arcLength = 0.65; // SASS
const svg = document.getElementById("speedometer");
const createElement = (type) => document.createElementNS("http://www.w3.org/2000/svg", type);
const createMainCircle = () => {
const cirMain = createElement("circle");
cirMain.setAttribute("cx", halfSize);
cirMain.setAttribute("cy", halfSize);
cirMain.setAttribute("r", mainRadius);
cirMain.setAttribute("stroke-width", (halfSize - mainRadius) * 2);
return cirMain;
};
const createPreviewCircle = () => {
const cirMain = createElement("circle");
cirMain.setAttribute("cx", halfSize);
cirMain.setAttribute("cy", halfSize);
cirMain.setAttribute("r", prevRadius);
cirMain.setAttribute("stroke-width", prevStroke);
return cirMain;
};
const cirBackground = svg.appendChild(createMainCircle());
cirBackground.id = "background";
svg.appendChild(cirBackground);
const cirProgress = svg.appendChild(createMainCircle());
cirProgress.id = "progress";
const groupText = svg.appendChild(createElement("g"));
[...Array(11).keys()].map((i) => {
const arcLengthRad = arcLength * Math.PI * 2;
const alfa = -arcLengthRad / 2 - 0.25 * Math.PI * 2 + (i * arcLengthRad) / 11 + arcLengthRad / 11 / 2;
const x = Math.round(Math.cos(alfa) * mainRadius + halfSize);
const y = Math.round(Math.sin(alfa) * mainRadius + halfSize);
const el = createElement("text");
el.textContent = i;
el.setAttribute("x", x);
el.setAttribute("y", y);
el.setAttribute("text-anchor", "middle");
el.setAttribute("dominant-baseline", "middle");
groupText.appendChild(el);
});
const groupHover = svg.appendChild(createElement("g"));
[...Array(11)].map(() => {
groupHover.appendChild(createMainCircle());
});
groupHover.id = "hover";
const groupPreview = svg.appendChild(createElement("g"));
[...Array(11)].map(() => {
groupPreview.appendChild(createPreviewCircle());
});
groupPreview.id = "preview";
};
const addListeners = (formId, selectName) => {
const svg = document.getElementById("speedometer");
let value = null;
[...document.getElementById("hover").children].map((item, i) => {
item.addEventListener("mouseenter", () => {
svg.classList.remove(...svg.classList);
svg.classList.add("is-" + i);
});
item.addEventListener("mouseleave", () => {
svg.classList.remove(...svg.classList);
if (value !== null) {
svg.classList.add("is-" + value);
}
});
item.addEventListener("click", () => {
value = i;
IWVA_runUserAction("valueChange", formId + ":" + selectName, value, false);
const submitButton = document.querySelector('#dc-output .iwsaas-form-submit');
submitButton.style.visibility = "visible";
submitButton.style.opacity = "1";
});
});
};
commandHook(NPSHack, function () {
addCustomOutputTextClass();
waitToElement('#dc-output form select', function () {
const formNPS = document.querySelector('#dc-output form');
formNPS.className = 'formNPS';
const selectNPS = formNPS.querySelector('select');
selectNPS.style.display = 'none';
if (!document.getElementById('speedometer')) {
const speedometerContainer = createElementAndAddAtribute('div', 'id', 'speedometerContainer');
const speedometrSvg = document.createElementNS("http://www.w3.org/2000/svg", "svg");
speedometrSvg.id = 'speedometer';
speedometrSvg.setAttribute("viewBox", "0 0 100 100");
speedometerContainer.appendChild(speedometrSvg);
selectNPS.after(speedometerContainer);
generateSvg();
addListeners(formNPS.id, selectNPS.name);
const containerHelp = createElementAndAddAtribute('div', 'className', 'containerHelp');
const helpZero = createElementAndAddAtribute('div', 'className', 'helpZero');
helpZero.innerText = '0 - not at all likely';
const helpLast = createElementAndAddAtribute('div', 'className', 'helpLast');
helpLast.innerText = '10 - extremely likely';
containerHelp.appendChild(helpZero);
containerHelp.appendChild(helpLast);
speedometerContainer.after(containerHelp);
}
addCancelButton('No, thanks', function () {
IWVA_openScript(25409);
});
}, 100);
});
