if (valueAsNode.children[j] && valueAsNode.children[j].classList.contains("ichr-output-text-question")) {
valueAsNode.children[j].classList.add("bot-msg");
valueAsNode.children[j].style.display = "none";
} else {
valueAsNode.children[j].style.display = "none";
}
}
value = valueAsNode.innerHTML;
} else {
if (/<b>Formula Finder:<\/b>/.test(value)) {
value = value.replace(/<b>Formula Finder:<\/b>/, `<b>Formula Finder:</b> <span class="ichr-output-text-question bot-msg" style="display: none">`) + '</span>';
} else {
for (const child of valueAsNode.children) {
child.style.display = "none"
}
value = valueAsNode.innerHTML;
}
}
return value;
}
const dcOutputHook = {
"type": "RenderText",
"properties": {
"dockerId": "dc-output",
"value": /.+[^"Loading\.\.\."]/
}
}
commandHook(dcOutputHook, function () {
const value = this.command.properties.value;
const newValue = separateQusetionsAndOthers(value);
this.command.properties.value = newValue;
});
const showWithDelay = (arrayOfElements, defaultTimeToDisplay) => {
if (arrayOfElements.length > 0) {
const currentElement = arrayOfElements.shift();
const typedMsg = document.getElementById("typedMsg");
//const defaultTimeToDisplay = 2000;
if (currentElement && currentElement.classList.contains("ichr-output-text-question")) {
currentElement.style.visibility = "hidden";
currentElement.style.transform = "rotateY(70deg) scaleY(0.5)";
if (typedMsg) typedMsg.style.display = "";
currentElement.style.display = "";
scrollToEnd();
setTimeout(function () {
if (typedMsg) typedMsg.style.display = "none";
currentElement.style.visibility = "";
currentElement.style.transform = "";
return showWithDelay(arrayOfElements, defaultTimeToDisplay);
}, defaultTimeToDisplay + currentElement.innerText.length);
} else {
setTimeout(function () {
if (typedMsg) typedMsg.style.display = "none";
currentElement.style.display = "";
scrollToEnd();
return showWithDelay(arrayOfElements, defaultTimeToDisplay);
}, defaultTimeToDisplay);
}
}
}
const isItAgainGetTime = (response, timeToDelayIfFalse) => {
const commandList = response.commandSetList[response.initialCommandSetId].commandDescList;
const procInfo = commandList.filter(o => o.processorId === "proc:info");
if (procInfo.length !== 0 && response.initialCommandSetId !== "set-icapi-action-mouseClick") {
return 0;
} else {
return timeToDelayIfFalse;
}
}
commandHookAll(dcOutputHook, function (e, response) {
const timeToDelay = isItAgainGetTime(response, 2000);
const ichrOutput = document.querySelector("#dc-output .ichr-output");
const observer = new MutationObserver(function (mutations) {
for (const mutation of mutations) {
if (!mutation.addedNodes) return
if (mutation.addedNodes.length > 0 && mutation.addedNodes[0].innerHTML) {
observer.disconnect();
setTimeout(function () {
const alldcOutputElements = Array.from(document.querySelectorAll("#dc-output .ichr-output-text > *"));
showWithDelay(alldcOutputElements, timeToDelay);
}, 100);
return
}
}
});
observer.observe(ichrOutput, {
childList: true,
subtree: true,
attributes: false,
characterData: false
});
});
const dcHistoryHook = {
"type": "RenderText",
"properties": {
"dockerId": "dc-history",
"value": /.+/
}
}
commandHook(dcHistoryHook, function () {
const command = this.command;
const valuesTab = [];
if (/ichr-output-text-question/.test(command.properties.value)) {
const nodeFromValue = document.createElement("span");
nodeFromValue.innerHTML = command.properties.value;
for (const child of nodeFromValue.children) {
if (child.classList.contains("ichr-output-text-question")) {
valuesTab.push("<b>Formula Finder:</b>" + child.outerHTML);
} else {
if (valuesTab[valuesTab.length - 1]) {
valuesTab[valuesTab.length - 1] += child.outerHTML;
}
}
}
command.properties.value = valuesTab[0];
for (let i = 1; i < valuesTab.length; i++) {
const newCommand = JSON.parse(JSON.stringify(command));
newCommand.properties.value = valuesTab[i];
newCommand.properties.id = this.command.properties.id + `-${i}`;
this.add(newCommand, this.index + i);
}
}
});
const scrollToEnd = () => {
const elmnt = document.getElementById("iw-output");
if (elmnt.scrollHeight > elmnt.clientHeight) {
requestAnimationFrame(function () {
let time = elmnt.scrollHeight - elmnt.scrollTop - elmnt.clientHeight + 70;
let curTime = 0;
function scrollSmoth() {
elmnt.scrollBy(0, 3);
}
while (curTime <= time) {
window.setTimeout(scrollSmoth, curTime);
curTime += 3;
}
});
}
}
const regexForFormulaFinder = {
start: /<span class=\"ffStart.*\"><\/span>/,
feedingIssues: /Does your baby have any of these/,
importantForYou: /<span class=\".*ffformula.*\">/,
preference: /<span class=\".*ffpreferences.*\">/,
results: /<span class=\".*ffproducts.*\".*>/
};
objICHR_InteliwiseSaaSModule.core.communicator.addEventListener('loadComplete', function (e) {
const response = e.source.getResponse();
const commandList = response.commandSetList[response.initialCommandSetId].commandDescList;
const commandListRenderText = commandList.filter(o => o.type === "RenderText");
const getformulaFinderHacks = () => {
const formulaFinderHacks = {};
for (let object in regexForFormulaFinder) {
formulaFinderHacks[object] = {
dcOutput: commandListRenderText.filter(command => regexForFormulaFinder[object].test(command.properties.value) && command.properties.dockerId === "dc-output"),
dcHistory: commandListRenderText.filter(command => regexForFormulaFinder[object].test(command.properties.value) && command.properties.dockerId === "dc-history"),
}
}
return formulaFinderHacks;
}
const formulaFinderHacks = getformulaFinderHacks();
if (formulaFinderHacks.results.dcHistory.length > 0) {
const resultsIds = formulaFinderHacks.results.dcHistory.map(com => com.properties.id);
resultsIds.map(id => {
waitToElement(`#${id}`, function () {
resultElement = document.getElementById(id);
if (!resultElement.classList.contains('formulaFinder')) {
iw.DOMFactory.addCSSClass(resultElement, 'formulaFinder ffResults');
addCaruselNav(`#${id} .ffproduct`, `carouselFor${id}`);
addCaruselNav(`#${id}-1 .ff-article`, `carouselArticleFor${id}`);
}
}, 100);
});
}
});
const findLastIdFromHistory = (response) => {
const findIdHistoryItems = response.commandSetList[response.initialCommandSetId].commandDescList.filter(c => c.type === "RenderText" && c.properties.dockerId === "dc-history");
if (findIdHistoryItems.length > 0) {
const lastIdInHistory = findIdHistoryItems[findIdHistoryItems.length - 1].properties.id;
return lastIdInHistory;
} else return false;
}
const addCustomOutputTextClass = () => {
const addClassToParents = () => {
const ichrOutputTextQuestions = document.querySelectorAll('#iw-output .ichr-output-text-question');
ichrOutputTextQuestions.forEach(ichrOutputTextQuestion => {
const ichrOutputTextQuestionParent = ichrOutputTextQuestion.parentElement;
if (!ichrOutputTextQuestionParent.classList.contains('custom-output-text')) iw.DOMFactory.addCSSClass(ichrOutputTextQuestionParent, 'custom-output-text');
});
}
waitToElement('#iw-output .ichr-output-text-question', function () {
addClassToParents();
setTimeout(addClassToParents, 200);
}, 100);
}
