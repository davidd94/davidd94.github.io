const createElementAndAddAtribute = (createElement, attributeToAdd, valueOfAttribute) => {
const element = document.createElement(createElement);
element[attributeToAdd] = valueOfAttribute;
return element;
}
const addTextBefore = (text) => {
const listContainer = document.querySelector('#dc-output .iw-sug').parentElement; //.parentElement;
listContainer.setAttribute('aria-label', text);
}
const waitToElement = (queryOfElement, functionToLunch, timeToCheck) => {
const interval = setInterval(function () {
const query = document.querySelector(queryOfElement);
if (query) {
clearInterval(interval);
functionToLunch();
}
}, timeToCheck);
}
const setProgressBar = (progressInPercent) => {
const avataBar = document.getElementById('iw-avatar-holder');
avataBar.style.borderImage = `linear-gradient(to right,  #00257a ${progressInPercent},  #dcdcdc ${progressInPercent}) 1`;
}
const slideOnTouch = (slider) => {
if (slider.children.length > 1) {
slider.style.placeContent = 'initial';
} else {
slider.style.placeContent = 'center';
}
slider.children[0].style.marginLeft = '20px';
slider.children[slider.children.length - 1].style.marginRight = '20px';
let isDown = false;
let startX;
let scrollLeft;
slider.addEventListener('mousedown', (e) => {
isDown = true;
startX = e.pageX - slider.offsetLeft;
scrollLeft = slider.scrollLeft;
});
slider.addEventListener('mouseleave', () => {
isDown = false;
});
slider.addEventListener('mouseup', (e) => {
isDown = false;
});
slider.addEventListener('mousemove', (e) => {
if (!isDown) return;
e.preventDefault();
const x = e.pageX - slider.offsetLeft;
const walk = (x - startX); //* 3;
slider.scrollLeft = scrollLeft - walk;
});
}
const buttonCarousel = (ulElement, idCarouselNav = 'carouselNav', numberSplitDots = 0) => {
const selectDot = (selected) => {
const allDots = document.querySelectorAll(`#${idCarouselNav} .dotForCarusel`);
for (let j = 0; j < allDots.length; j++) {
if (j === selected) {
allDots[j].style.backgroundColor = '#00257a';
} else {
allDots[j].style.backgroundColor = '';
}
}
}
const allIwSug = ulElement.querySelectorAll('.iw-sug');
ulElement.style.placeContent = 'center';
let selectedDot = 0;
const addNav = (restartPosition = false) => {
const howManyInView = numberSplitDots === 0 ? Math.floor((ulElement.offsetWidth - 20) / ulElement.children[0].offsetWidth) : numberSplitDots;
const floorHowMany = Math.floor(ulElement.children.length / howManyInView);
const amoutDot = ulElement.children.length % howManyInView === 0 ? floorHowMany : floorHowMany + 1; //ulElement.children.length if one
const moveCaruselTo = (toShow) => {
const isItLastDot = (amoutDot - 1) * howManyInView === toShow && toShow !== 0;
const lastElement = ulElement.children[ulElement.childElementCount - 1];
const forLastDotFromLeft = -(lastElement.offsetLeft + lastElement.offsetWidth - ulElement.offsetWidth + 20);
const elementsFromLeft = -(ulElement.children[0].offsetWidth + 12) * toShow;
const fromLeftElements = isItLastDot ? forLastDotFromLeft : elementsFromLeft;
const centerElements = ulElement.offsetWidth / 2 - ulElement.children[0].offsetWidth / 2 - 20 + elementsFromLeft;
const withTranslateX = howManyInView > 1 ? fromLeftElements : centerElements;
allIwSug.forEach(iwSug => {
iwSug.style.transform = `translateX(${withTranslateX}px)`;
});
}
if (restartPosition) moveCaruselTo(0);
if (amoutDot > 1) {
allIwSug[0].style.marginLeft = '20px';
allIwSug[allIwSug.length - 1].style.marginRight = '20px';
ulElement.style.placeContent = 'initial';
const navigationBar = createElementAndAddAtribute('div', 'id', idCarouselNav);
navigationBar.className = 'carouselNav';
const arrowLeft = createElementAndAddAtribute('div', 'className', 'arrowLeft');
arrowLeft.innerHTML = '<img src="http://inteliwise-eu.s3.amazonaws.com/Enfamil/arrowLeft.png">';
arrowLeft.addEventListener('click', function () {
if (selectedDot > 0) {
selectedDot--;
}
const elementToSelect = selectedDot * howManyInView;
moveCaruselTo(elementToSelect);
selectDot(selectedDot);
});
navigationBar.appendChild(arrowLeft);
for (let i = 0; i < amoutDot; i++) {
const iwSugDot = createElementAndAddAtribute('div', 'className', 'dotForCarusel');
iwSugDot.classList.add('iwdot' + (i + 1));
iwSugDot.addEventListener('click', function () {
const toSelect = i * howManyInView;
moveCaruselTo(toSelect);
selectedDot = i;
selectDot(selectedDot);
});
navigationBar.appendChild(iwSugDot);
}
const arrowRight = createElementAndAddAtribute('div', 'className', 'arrowRight');
arrowRight.innerHTML = '<img src="http://inteliwise-eu.s3.amazonaws.com/Enfamil/arrowRight.png">';
arrowRight.addEventListener('click', function () {
if (selectedDot < amoutDot - 1) {
selectedDot++;
}
const elemetToSelect = selectedDot * howManyInView;
moveCaruselTo(elemetToSelect);
selectDot(selectedDot);
})
navigationBar.appendChild(arrowRight);
if (!document.getElementById(idCarouselNav)) {
ulElement.after(navigationBar);
//IWVA_scrollOutput();
scrollToEnd();
}
selectDot(selectedDot);
moveCaruselTo(selectedDot);
}
}
const interval = setInterval(function () {
if (ulElement.offsetWidth > 0) {
clearInterval(interval);
addNav();
}
}, 100);
function refreshIfResize() {
const caruselDiv = document.getElementById('carouselNav');
if (caruselDiv) caruselDiv.remove();
selectedDot = 0;
addNav(true);
}
window.addEventListener('resize', refreshIfResize);
}
const addCaruselNav = (childQuery = '#dc-output .iw-sug', idCarouselNav = 'carouselNav') => {
waitToElement(childQuery, function () {
const ulElement = document.querySelector(childQuery).parentElement;
if (!iw.mobile) {
buttonCarousel(ulElement, idCarouselNav);
} else {
slideOnTouch(ulElement);
}
}, 300);
}
const addCancelButton = (textForCancel, functionToLunch, elemBeforeToAdd) => {
if (!document.querySelector('.iwsaas-form-cancel')) {
elemBeforeToAdd = elemBeforeToAdd ? elemBeforeToAdd : document.querySelector('.iwsaas-form-submit');
const cancelButton = createElementAndAddAtribute('a', 'className', 'iwsaas-form-cancel');
cancelButton.innerText = textForCancel;
cancelButton.addEventListener('click', functionToLunch);
elemBeforeToAdd.after(cancelButton);
setTimeout(function () {
//IWVA_scrollOutput()
scrollToEnd();
}, 500);
}
}
waitToElement('#dc-zendesk-support .ichr-caption', function () {
document.querySelector('#dc-zendesk-support .ichr-caption').innerText = "Contact Us";
}, 100);
const basedOnYourAnswerFunc = () => {
addTextBefore('Select up to 2 answers');
const checkBoxes = document.querySelectorAll('#dc-output .checkbox-rb-enfamil');
let checkedCount = 0;
const doneBtn = document.querySelector('#dc-output button.iw-sug:last-of-type');
//doneBtn.scrollIntoView();
doneBtn.style.visibility = 'hidden';
doneBtn.style.opacity = 0;
checkBoxes.forEach(chkbox => chkbox.addEventListener('change', function () {
if (chkbox.checked) {
checkedCount++;
chkbox.parentElement.classList.add('checkbox-chceked');
doneBtn.style.visibility = '';
doneBtn.style.opacity = '';
if (checkedCount === 2) {
checkBoxes.forEach(cbox => {
if (!cbox.checked) cbox.disabled = true;
});
}
} else {
checkedCount--;
chkbox.parentElement.classList.remove('checkbox-chceked');
checkBoxes.forEach(cbox => cbox.disabled = false);
if (checkedCount === 0) {
doneBtn.style.visibility = 'hidden';
doneBtn.style.opacity = 0;
}
}
}));
addCaruselNav();
//scrollToEnd();
}
