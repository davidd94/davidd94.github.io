const theyMayGrowFast = {
"processorId": "proc-iwsaas-render",
"type": "RenderText",
"synchronous": false,
"waitAfter": -1,
"properties": {
"dockerId": "dc-output",
"value": regexForFormulaFinder.start, ///<b>Formula Finder:<\/b> They may grow fast, but how old is your little one right now/,
"clearBefore": true
}
}
commandHook(theyMayGrowFast, function (a, commandList) {
const idLastHistory = findLastIdFromHistory(commandList);
if (idLastHistory) {
waitToElement(`#${idLastHistory}`, function () {
sessionBreak('New session', idLastHistory);
}, 100);
}
