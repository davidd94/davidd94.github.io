const getDataset = () => {
const outputFirstSpan = document.querySelector('#dc-output .ichr-content > .ichr-output-text > .ffproducts');
const dataset = outputFirstSpan && outputFirstSpan.dataset ? JSON.parse(outputFirstSpan.dataset.carousel) : [];
const formatArrayToDataLayer = (array) => array.map((e, index) => ({
"list": "formulafinder",
"id": window.atob(e.id).split('/').pop(),
"name": e.text,
"position": index + 1,
"category": "formula"
}));
if (dataset.length) {
const formatedData = formatArrayToDataLayer(dataset)
pushToDataLayer('eeListViewFormulaFinder', {
products: formatedData
});
}
}
const pushResultToDataLayer = (event, id, text, category, index) => {
const result = [{
"list": "formulafinder",
"id": window.atob(id).split('/').pop(),
"name": text,
"position": index,
"category": "formula"
}]
pushToDataLayer(event, {
products: result
});
}
commandHook(productCarousel, function () {
waitToElement('#dc-output .ichr-content > .ichr-output-text', getDataset, 100);
});
