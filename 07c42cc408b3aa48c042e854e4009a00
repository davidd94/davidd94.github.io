const addToolTip = () => {
if (!document.getElementById('backfade')) {
const iwModMid = document.getElementById('iw-module-wrapper-middle');
const backfade = createElementAndAddAtribute('div', 'id', 'backfade');
const toolTip = createElementAndAddAtribute('div', 'id', 'toolTip');
const contentUl = createElementAndAddAtribute('ul', 'id', 'contentContainer');
const closeTooltip = createElementAndAddAtribute('div', 'id', 'closeNotSure');
closeTooltip.innerHTML = 'Close';
for (let position in toolTipContent) {
const liElem = createElementAndAddAtribute('li', 'className', 'toolTiplist');
liElem.innerHTML = toolTipContent[position];
contentUl.appendChild(liElem);
}
closeTooltip.addEventListener('click', function () {
backfade.removeAttribute("class");
});
backfade.addEventListener('click', function (e) {
if (e.target === backfade) closeTooltip.click()
});
toolTip.appendChild(contentUl);
toolTip.appendChild(closeTooltip);
backfade.appendChild(toolTip);
iwModMid.appendChild(backfade);
}
}
addNoSure();
addToolTip();
}
const iamNotSureToolTipText = [
`<b>Gas and Fussiness</b> <br>Baby cries and is fussy for an hour or so a day; isn't eating or sleeping well;
squirms as though uncomfortable and pulls legs up to chest.`,
`<b>Crying or Colicky</b>  <br>Your baby's crying totals at least three hours a day; occurs at least three days a 
week; persists for at least three weeks in a row.`,
`<b>Spitup</b> <br>Baby mildly throws up food, milk, and saliva. 
This usually occurs right after feeding or burping.`,
`<b>Occasional Constipation and Hard Stool</b> <br>Baby goes more than a few days without a bowel movement, and then has a hard stool.`,
`<b>Lactose Sensitivity</b> <br>Baby has abdominal pain, diarrhea, nausea, gassiness or bloating after drinking milk or formula that contains lactose.`,
`<b>Cow's Milk and Other Food Allergies</b> <br>Baby has digestion problems, including tummy ache, diarrhea, and constipation;
skin reactions, such as swelling of the lips, face, and eye area, and a red, itchy rash; Hay-fever-like symptoms, 
such as a runny nose and watery eyes; eczema that doesn't get better with treatment.`
]
const getLastQuestion = (response) => {
const regexReplacer = (string) => {
return string.replace(/(\r\n|\n|\r)/gm, "").replace(/ +(?= )/g, '')
}
const formatQuestion = (string) => {
if (string.querySelectorAll('.ichr-output-text-question').length) {
return regexReplacer(Array.from(string.getElementsByClassName('ichr-output-text-question')).map(e => e.innerText).join(' '))
}
return regexReplacer(string.innerText.replace("Formula Finder: ", ""));
}
const formatAnswer = (string) => {
return regexReplacer(string.innerText.replace("You: ", ""));
}
const questionAnswer = response.commandSetList[response.initialCommandSetId].commandDescList.filter(c => c.type === "RenderText" && c.properties.dockerId === "dc-history")
const question = document.createElement('div');
const answer = document.createElement('div');
questionAnswer.forEach((e) => {
if (e.properties.cssClass) e.properties.cssClass.includes('iwsaas-user') ? answer.innerHTML = e.properties.value : question.innerHTML = e.properties.value;
})
pushToDataLayer("formulaFinderStep", {
"formulaFinderAnswer": formatAnswer(answer),
"formulaFinderStep": formatQuestion(question)
})
};
