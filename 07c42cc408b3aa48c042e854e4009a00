const baseUrl = iwsaas.applicationPath.split('/proxy')[0];
const url = new URL(`${baseUrl}/services/gateway/v_3.0/`);
const getData = async (uri, data) => {
Object.keys(data).forEach((key) =>
uri.searchParams.append(key, JSON.stringify(data[key])),
);
const response = await fetch(uri);
return response.json();
};
const params = {
applicationPath: iwsaas.applicationPath,
installationId: iwsaas.installationId,
layoutMode: iwsaas.layoutMode,
align: iwsaas.align,
lang: iwsaas.lang,
hidden: iwsaas.hidden,
}
const config = {
saasapiVer: '1.5',
}
const getCookieValue = (name) => {
var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
return v ? v[2] : null;
}
const getValuesFromDataLayer = () => ({
loginStatus: dataLayer[0].loginStatus,
pageTemplate: dataLayer[0].pageTemplate,
pagePrimaryNav: dataLayer[0].pagePrimaryNav,
userId: dataLayer[0].userId,
});
const pushToDataLayer = (event, value) => {
if (window.dataLayer) {
window.dataLayer.push({
event,
clientId: getCookieValue('_ga'),
...value,
...getValuesFromDataLayer()
});
}
};
const IWVA_triggerWidgetFromSuggestedValue = async (value) => {
const getCookie = () => {
return document.cookie
.split(';')
.filter((e) => e.includes('ichr-session'))[0]
}
if (!iw.saasModule.getSession()) {
await IWVA_getFirstQuestion();
}
const getSessionId = (callback) => new Promise(
resolve => {
const interval = setInterval(() => {
const session = callback();
if (session) {
clearInterval(interval);
return resolve(session);
}
}, 100);
},
reject => reject(console.log("There is problem to get sessionId")));
const sessionId = await getSessionId(iw.saasModule.getSession);
const queryParams = {
params,
config,
request: {
causeActionType: 'mouseClick',
causeElementId: 'dc-home-button',
isFile: '',
greedy: true,
hooksDone: true,
assistantId: 'Inteliwise/SaaSModule_1_5/index',
sessionId: sessionId,
init: false,
},
};
const result = await getData(url, queryParams);
const suggestedIds = result.commandSetList[
'set-icapi-action-mouseClick'
].commandDescList.filter((e) => e.type === 'RenderLink');
const filterSuggested = suggestedIds.filter((e) => e.properties.value === value);
if (filterSuggested.length) {
IWVA_expand();
IWVA_runUserAction(
'suggestedAnswer',
filterSuggested[0].properties.id,
filterSuggested[0].properties.value,
true,
);
if (filterSuggested[0].properties.value.length) {
pushToDataLayer('formulaFinderChatOpen', {
formulaFinderAnswer: filterSuggested[0].properties.value.split("<br>")[0]
});
}
}
};
const IWVA_getFirstQuestion = async () => {
IWVA_showWelcome();
const queryParams = {
params,
config,
request: {
causeActionType: 'callbackExternal',
causeElementId: 'showWelcome',
greedy: true,
hooksDone: true,
assistantId: 'Inteliwise/SaaSModule_1_5/index',
revision: '1-3256936845',
flashVersion: '-1',
sessionId: null,
jsResponseSetter: null,
init: true,
},
};
const result = await getData(url, queryParams);
const suggestedIds = result.commandSetList[
'iwsaas-cmd-after-init'
].commandDescList.filter((e) => e.type === 'RenderLink');
const question = result.commandSetList[
'iwsaas-cmd-after-init'
].commandDescList
.filter(
(e) => e.type === 'RenderText' && e.properties.dockerId === 'dc-output',
)
.filter((e) => !suggestedIds.some((f) => e.properties.value.includes(f)))[0]
.properties.value.replace(/<b>Formula Finder:<\/b> /g, '');
const suggested = result.commandSetList[
'iwsaas-cmd-after-init'
].commandDescList
.filter(
(e) => e.type === 'RenderLink' && e.properties.dockerId === 'dc-output',
)
.map((e) => e.properties.value);
return {
question,
suggested
};
}
