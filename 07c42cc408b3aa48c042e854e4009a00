const balloonShowOrHide = (showBalloon) => {
const dcTabButton = document.getElementById('dc-tab-button');
const dcBalloon = document.getElementById('dc-balloon');
if (showBalloon) {
dcTabButton.style.display = 'block';
dcBalloon.style.display = '';
} else {
dcTabButton.style.display = '';
dcBalloon.style.display = 'none';
}
}
const addFuncToBalloonClick = (tabCollapse, firstText, secondText, timeToHide = -1, lunchFunction = false) => {
let timer;
const closeButtonOnHaburgerMenu = document.querySelector('#__next .mobile-nav.mobile-nav--open + button.mobile-nav__close-button');
if (closeButtonOnHaburgerMenu) {
closeButtonOnHaburgerMenu.click();
}
changeBaloonText(firstText, secondText);
tabCollapse ? IWVA_collapse() : '';
balloonShowOrHide(true);
const tempFunc = (e) => {
e.stopImmediatePropagation();
clearTimeout(timer);
balloonShowOrHide(false);
if (lunchFunction) {
lunchFunction();
} else {
IWVA_expand();
}
}
const dcBalloon = document.getElementById('dc-balloon');
dcBalloon.addEventListener('click', tempFunc, {
once: true
});
if (timeToHide > -1) {
timer = setTimeout(function () {
dcBalloon.removeEventListener('click', tempFunc);
balloonShowOrHide(false);
}, timeToHide);
}
}
