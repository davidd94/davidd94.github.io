const hasYourBaby = {
"processorId": "proc-iwsaas-render",
"type": "RenderText",
"synchronous": false,
"waitAfter": 0,
"properties": {
"dockerId": "dc-output",
"value": regexForFormulaFinder.feedingIssues,
"clearBefore": true
