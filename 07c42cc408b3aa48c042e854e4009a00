changeToInputButton = () => {
const iwInput = document.querySelector('#dc-input .ichr-input')
iwInput.placeholder = 'Type your message...'
const dcTab = document.getElementById('dc-tab-button');
iwInput.addEventListener('input', function (event) {
if (iwInput.value.trim() && event.inputType !== 'insertLineBreak') {
dcTab.style.zIndex = '16777261';
} else {
dcTab.style.zIndex = '16777262';
}
});
iwInput.addEventListener('focus', function (event) {
if (!event.sourceCapabilities) {
dcTab.style.zIndex = '16777262';
}
});
}
const showSubmitTwoInput = () => {
waitToElement("#dc-output form input", function () {
addCancelButton('Cancel', IWVA_showWelcome); //only for custom contact
const inputForm = document.querySelectorAll("#dc-output form input");
for (let i = 0; i < 2; i++) {
inputForm[i].addEventListener('input', function () {
const submitButton = document.querySelector('#dc-output .iwsaas-form-submit');
if (inputForm[0].value.trim() && inputForm[1].value.trim()) {
submitButton.style.visibility = "visible";
submitButton.style.opacity = "1";
} else {
submitButton.style.visibility = "";
submitButton.style.opacity = "";
}
});
}
}, 100);
}
